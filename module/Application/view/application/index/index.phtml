<script type="text/javascript">
    $(document).ready(function () {
        app.hometesticarousal = 1;
        app.hometesticarousalnext = 2;
        app.hometesticarousalprev = 0;
        app.hometesticarusallength = $('.mkdf-testimonial-content').length; 
        app.hometestimoniallength = $('.testimonials-v6').length;
        app.hometestiperpage = 3;
        app.hometestipages = parseInt(app.hometestimoniallength/app.hometestiperpage)+app.hometestimoniallength%app.hometestiperpage;
        app.hometesticurpage = 0;
        
        
        showtestimonial();
        showhometeam();
        $('.right-arrow').click(function(){
            if(app.hometesticarousal < app.hometesticarusallength){
                app.hometesticarousalnext = app.hometesticarousal + 1;
                app.hometesticarousal = app.hometesticarousal + 1;
            }
            showtestimonial();
        });
        
        $('.left-arrow').click(function(){  
            if(app.hometesticarousal > 1){
                app.hometesticarousalprev = app.hometesticarousal - 1;
                app.hometesticarousal = app.hometesticarousal - 1;
            }
            showtestimonial();
        });
        
        $('.white-right-arrow').click(function(){  
            if(app.hometesticurpage < app.hometestipages){                
                app.hometesticurpage = app.hometesticurpage + 1;
            }
            showhometeam();
        });
        
        $('.white-left-arrow').click(function(){  
            if(app.hometesticurpage >= 1){                
                app.hometesticurpage = app.hometesticurpage - 1;
            }
            showhometeam();
        });
    });
    
    function showtestimonial(){        
        $('.mkdf-testimonial-content').each(function(){            
            $(this).hide();
        });
        $('#testi-'+app.hometesticarousal).show();
    }
    
    function showhometeam(){     
        console.log(app);
        $('.testimonials-v6').hide();
        
        var start = app.hometesticurpage + 1;
        var last = app.hometesticurpage + app.hometestiperpage
        for(var i = start; i <= last ; i++){
            $('#team-'+i).show();
        }
    }
</script>